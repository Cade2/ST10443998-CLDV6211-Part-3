@model ST10443998_CLDV6211_POE.Models.Venue

@{
    ViewData["Title"] = "Delete Venue";
}

<h2>Confirm Delete</h2>

<div class="alert alert-danger">
    <strong>Are you sure you want to delete this venue?</strong>
</div>

<dl class="row">
    <dt class="col-sm-2">Venue ID</dt>
    <dd class="col-sm-10">@Model.VenueId</dd>

    <dt class="col-sm-2">Venue Name</dt>
    <dd class="col-sm-10">@Model.VenueName</dd>
</dl>

<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="VenueId" />
    <button type="submit" class="btn btn-danger">Delete</button>
</form>
